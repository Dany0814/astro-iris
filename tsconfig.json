{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
      "importsNotUsedAsValues": "error",
      "baseUrl": ".",
      "paths": {
        "@components/*": ["src/components/*"],
        "@layouts/*": ["src/layouts/*"],
        "@utils/*": ["src/utils/*"],
      }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}