{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
      "types": ["astro/client"],
      "importsNotUsedAsValues": "error",
      "baseUrl": ".",
      "paths": {
        "@components/*": ["src/components/*"],
        "@layouts/*": ["src/layouts/*"],
        "@utils/*": ["src/utils/*"],
        "@icons/*": ["./node_modules/.pnpm/@tabler+icons@1.113.0/node_modules/@tabler/icons/icons/*"]
      }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}